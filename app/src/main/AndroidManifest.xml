<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/loading"
        android:label="SMKN1TANJUNGPANDAN"
        android:roundIcon="@drawable/loading"
        android:supportsRtl="true"
        android:theme="@style/Theme.CompanyProfile"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".JurusanActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".StructureActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".PrestasiActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".GalleryActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ServiceActivity"
            android:exported="false"
            android:label="@string/title_activity_service"
            android:theme="@style/Theme.CompanyProfile.NoActionBar" />
        <activity
            android:name=".WebActivity"
            android:configChanges="orientation|screenSize|keyboard"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|keyboard"
            android:exported="false"
            android:label="@string/app_name"
            android:theme="@style/Theme.CompanyProfile.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>